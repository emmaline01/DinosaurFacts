{
  "name": "Daily Dinos",
  "description": "A new random dinosaur every day!",
  "version": "1.0",
  "manifest_version": 3,
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "host_permissions": [
    "https://dinosaur-facts-api.shultzlab.com/*",
    "https://en.wikipedia.org/w/*",
    "https://paleobiodb.org/*"
  ],
  "permissions": [
    "storage"
  ],
 "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
